{
  "name": "bloomquiz",
  "version": "0.0.1",
  "scripts": {
    "prep": "npm i && npm run resetdb",
    "frontend": "webpack serve --open",
    "backend": "nodemon index.js",
    "dev": "fkill :9000 :3000 -s && concurrently --handle-input \"npm:backend\" \"npm:frontend\"",
    "backend:test": "cross-env NODE_ENV=testing nodemon index.js",
    "jest:test": "cross-env NODE_ENV=testing jest --runInBand",
    "test": "fkill :9000 -s && concurrently --handle-input -n api,test --kill-others \"npm:backend:test\" \"npm:jest:test\"",
    "start": "node index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "ketchup": "git fetch && git reset --hard origin/lecture && npm run prep"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "fkill-cli": "^7.1.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "msw": "^0.42.0",
    "nodemon": "^2.0.16",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^6.0.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "sqlite3": "^5.0.8",
    "styled-components": "^5.3.5",
    "yup": "^0.32.11"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/bloomquiz_solution.git"
  }
}
